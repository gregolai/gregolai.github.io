	"main": "main.js",
	"scripts": {
		"build": "npm run build:html && npm run build:styles",
		"build:html": "npx static-react src/index.js > public/index.html",
		"build:styles": "npx sass --no-source-map src/main.scss public/main.css",
		"run": "open index.html && npm run watch",
		"watch": "npx watch \"npm run build\" ./src"
	},
