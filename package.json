	"dependencies": {
		"react": "^16.8.2",
		"react-dom": "^16.8.2",
		"static-react": "^4.0.0-3",
		"styled-components": "^4.1.3",
		"styled-normalize": "^8.0.6",
		"styled-reset": "^1.6.8"
	},
	"devDependencies": {
		"sass": "^1.23.0"
	}
